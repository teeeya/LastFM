{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.last.fm/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Login"
      }
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": ""
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "login"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Logout"
      }
    }
  ]
}
